{{#if carousel.hasCarousel}}
    <div id="carousel-container" class="carousel-container"></div>
{{/if}}
</div>
<div id="course-body" class="course-body"></div>
{{#if ShowLauncher}}
    <div class="launch-holder">
        <div class="launch-title">
            <div class="logo"><img src="resources/image/{{logo}}" onload="SVGInject(this)" /></div>
            <div class='course-title'>{{{title}}}</div>
        </div>
        <div class='launch-button'>Launch</div>
    </div>
{{/if}}